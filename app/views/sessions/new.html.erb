<h1> Log in to Running Club </h1>
<%= form_for @user, url: '/login' do |f| %>
  Email: <%= f.text_field :email, required: true %> </br>
  Password: <%= f.password_field :password, required: true %> </br>
  <%= f.submit "Login" %>
<% end %>
</br>

<%= link_to('Log in with Google!', '/auth/google_oauth2') %> |
<%= link_to "Sign Up", new_user_path %>
