<%= form_for @route do |f| %>
  Title: <%= f.text_field :title %> </br>
  Location: <%= f.text_field :location %> </br>
  Distance (miles): <%= f.number_field :distance %> </br>
  Elevation (feet): <%= f.number_field :elevation %> </br>
  Discription: <%= f.text_field :description %> </br>
  <%= f.submit "Submit" %>
<% end %>
